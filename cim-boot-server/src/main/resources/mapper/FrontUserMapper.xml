<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.farsunset.cim.dao.FrontUserMapper">


	<!-- 获取 -->
	<select id="findById" resultType="com.farsunset.cim.entity.FrontUserDTO">
		SELECT
		front_user_id,
		user_name,
		phone
		FROM
			zlp_front_user
		WHERE
			front_user_id = #{userId}
	</select>


	
</mapper>